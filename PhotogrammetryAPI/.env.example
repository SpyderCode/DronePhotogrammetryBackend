# Photogrammetry API Configuration Example
# Copy this file to appsettings.Development.json or appsettings.Production.json

# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_NAME=photogrammetry
DATABASE_USER=root
DATABASE_PASSWORD=your_password_here

# JWT Authentication
JWT_SECRET_KEY=YourSuperSecretKeyThatIsAtLeast32CharactersLong!
JWT_ISSUER=PhotogrammetryAPI
JWT_AUDIENCE=PhotogrammetryAPI

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest

# Storage Configuration
STORAGE_UPLOADS_PATH=uploads
STORAGE_MODELS_PATH=models

# Meshroom Configuration (optional)
MESHROOM_BATCH_PATH=meshroom_batch
MESHROOM_EXECUTABLE_PATH=meshroom_photogrammetry

# API Configuration
BASE_URL=http://localhost:5000
MAX_UPLOAD_SIZE_MB=10240

# Docker Setup (for docker-compose)
MYSQL_ROOT_PASSWORD=rootpassword
